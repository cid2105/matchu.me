$("img[center]").attr("uid", "<%= @center_image_uid %>")
				.attr("src", "http://graph.facebook.com/<%= @center_image_uid %>/picture?type=large")
				.attr("data-original-title", "<%= @user %>")
				.attr("title", "<%= @user %>")
				.attr("data-content", "<%= @user.education %><br/><br/><%= @user.major %>");

$(".polaroid h6").text("<%= @user %> ");
$("ul.thumbnails").html("<%= j render "home/thumbs", :thumb_uids => @thumb_uids %>");
